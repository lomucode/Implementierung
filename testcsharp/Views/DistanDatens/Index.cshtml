@model IEnumerable<testcsharp.models.DistanDaten>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Distanz)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DistanzArt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Aktivitätsdauer)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DistanzManuellAnpassen)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DistanDatenDatum)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Distanz)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DistanzArt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Aktivitätsdauer)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DistanzManuellAnpassen)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DistanDatenDatum)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
